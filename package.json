    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-reflex": "^2.0.11",
    "react-splitter-layout": "^3.0.0",
    "react-swipeable-views": "^0.12.12",
    "redux": "^3.7.2",
    "typed-rest-client": "^0.15.1",
    "typeface-roboto": "0.0.45"

{
  "name": "pedalpro-studio",
  "description": "PedalPro Studio",
  "version": "0.3.0",
  "author": {
    "email": "obiwanjacobi@hotmail.com",
    "name": "Jacobi Software",
    "url": "https://github.com/obiwanjacobi/PedalPro-Studio/wiki"
  },
  "main": "./app/Startup.js",
  "private": true,
  "scripts": {
    "build": "./node_modules/.bin/webpack --config webpack.dev.config.js",
    "build:prod": "./node_modules/.bin/webpack --config webpack.prod.config.js",
    "build:prod:prog": "./node_modules/.bin/webpack --config webpack.prod.prog.config.js",
    "build:prod:app": "./node_modules/.bin/webpack --config webpack.prod.app.config.js",
    "test": "jest",
    "test-electron": "SET ELECTRON_RUN_AS_NODE=true ./node_modules/.bin/electron ./node_modules/.bin/jest",
    "test-w": "SET ELECTRON_RUN_AS_NODE=true ./node_modules/.bin/electron ./node_modules/.bin/jest --watchAll",
    "start": "./node_modules/.bin/electron .",
    "postinstall": "./node_modules/.bin/electron-builder install-app-deps && ./node_modules/.bin/tsc && ./node_modules/.bin/electron-rebuild.cmd --force",
    "build:native": "./node_modules/.bin/electron-rebuild.cmd --force",
    "release": "./node_modules/.bin/electron-builder"
  },
  "dependencies": {
    "@material-ui/core": "^3.2.0",
    "@material-ui/icons": "^3.0.1",
    "@material-ui/lab": "^3.0.0-alpha.19",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "fs": "0.0.1-security",
    "lodash": "^4.17.11",
    "mdi-material-ui": "^5.5.0",
    "mdi-react": "^4.4.0",
    "mkdirp": "^0.5.1",
    "morgan": "^1.9.1",
    "node-hid": "^0.7.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-virtualized": "^9.20.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "typed-rest-client": "^1.0.9",
    "typeface-roboto": "^0.0.54"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.5",
    "@types/lodash": "^4.14.117",
    "@types/mkdirp": "^0.5.2",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.11.7",
    "@types/node-hid": "^0.7.0",
    "@types/prop-types": "^15.5.6",
    "@types/react": "^16.4.16",
    "@types/react-dom": "^16.0.9",
    "@types/react-redux": "^6.0.9",
    "@types/react-virtualized": "^9.18.7",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "ajv": "^6.5.4",
    "ajv-keywords": "^3.2.0",
    "electron": "^3.0.4",
    "electron-builder": "^20.28.4",
    "electron-rebuild": "^1.8.2",
    "jest": "^23.6.0",
    "node-loader": "^0.6.0",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.2.1",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.3",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "windows-build-tools": "^5.0.0"
  },
  "build": {
    "appId": "jacobi.pedalpro.studio",
    "mac": {
      "category": "public.app.category.productivity"
    },
    "copyright": "Copyright Â© 2018 Jacobi Software",
    "productName": "PedalPro Studio",
    "asar": "true",
    "files": [
      "./app/**/*.js",
      "./app/**/*.json",
      "./app/**/*.css",
      "./app/**/*.html",
      "./src/**/*.css",
      "./src/**/*.html",
      "./assets/*.*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "release"
    },
    "fileAssociations": {
      "ext": "ppp",
      "name": "PedalPro-Preset",
      "description": "A (JSON) file containing a PedalPro Preset",
      "role": "none"
    }
  }
}

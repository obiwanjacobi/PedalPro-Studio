{
  "name": "pedalpro-studio",
  "description": "PedalPro Studio",
  "version": "0.2.1",
  "author": {
    "email": "obiwanjacobi@hotmail.com",
    "name": "Jacobi Software",
    "url": "https://www.vintage-revolution.com"
  },
  "main": "./app/program.js",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^1.0.0",
    "@material-ui/icons": "^1.0.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "fs": "0.0.1-security",
    "immutable": "^3.8.2",
    "lodash": "^4.17.10",
    "mdi-material-ui": "^5.0.0",
    "mdi-react": "^3.3.0",
    "mkdirp": "^0.5.1",
    "morgan": "^1.9.0",
    "node-hid": "^0.7.2",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-virtualized": "^9.19.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "typed-rest-client": "^1.0.7",
    "typeface-roboto": "^0.0.54"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/jest": "^22.2.3",
    "@types/lodash": "^4.14.109",
    "@types/mkdirp": "^0.5.2",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.1.2",
    "@types/node-hid": "^0.5.2",
    "@types/prop-types": "^15.5.3",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^6.0.0",
    "@types/react-virtualized": "^9.18.3",
    "@types/redux-thunk": "^2.1.32",
    "ajv": "^6.0.0",
    "ajv-keywords": "^3.2.0",
    "electron": "^1.8.4",
    "electron-builder": "^20.14.7",
    "electron-rebuild": "^1.7.3",
    "jest": "^23.0.0",
    "node-loader": "^0.6.0",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.3.0",
    "tslint": "^5.10.0",
    "tslint-loader": "^3.6.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.8.3",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8",
    "windows-build-tools": "^2.3.0"
  },
  "scripts": {
    "build": "./node_modules/.bin/webpack --config webpack.dev.config.js --mode development",
    "build:prod": "./node_modules/.bin/webpack --config webpack.prod.config.js --mode production",
    "test": "SET ELECTRON_RUN_AS_NODE=true ./node_modules/.bin/electron ./node_modules/.bin/jest",
    "test-w": "SET ELECTRON_RUN_AS_NODE=true ./node_modules/.bin/electron ./node_modules/.bin/jest --watchAll",
    "start": "./node_modules/.bin/electron .",
    "postinstall": "./node_modules/.bin/electron-builder install-app-deps && ./node_modules/.bin/tsc && ./node_modules/.bin/electron-rebuild.cmd --force",
    "build:native": "./node_modules/.bin/electron-rebuild.cmd --force",
    "release": "./node_modules/.bin/electron-builder"
  },
  "build": {
    "appId": "jacobi.pedalpro.studio",
    "mac": {
      "category": "public.app.category.productivity"
    },
    "copyright": "Copyright Â© 2018 Jacobi Software",
    "productName": "PedalPro Studio",
    "asar": "true",
    "files": [
      "./app/**/*.js",
      "./app/**/*.json",
      "./app/**/*.css",
      "./app/**/*.html",
      "./src/**/*.css",
      "./src/**/*.html",
      "./assets/*.*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "release"
    },
    "fileAssociations": {
      "ext": "ppp",
      "name": "PedalPro-Preset",
      "description": "A (JSON) file containing a PedalPro Preset",
      "role": "none"
    }
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    },
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(\\.(test|spec))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
